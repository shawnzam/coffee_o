<h1>Listing coffee_drinkers</h1>

<table>
  <thead>
    <tr>
      <th>Phone number</th>
      <th>Current score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coffee_drinkers.each do |coffee_drinker| %>
      <tr>
        <td><%= coffee_drinker.phone_number %></td>
        <td><%= coffee_drinker.current_score %></td>
        <td><%= link_to 'Show', coffee_drinker %></td>
        <td><%= link_to 'Edit', edit_coffee_drinker_path(coffee_drinker) %></td>
        <td><%= link_to 'Destroy', coffee_drinker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coffee drinker', new_coffee_drinker_path %>
